{
    "langrev": "1",
    "warnings": ["no-unused"],
    "dependency-search-paths": ["lib"],
    "dependencies": ["raylib55", "novacrash", "nk", "unuv", "raygui"],
    "authors": ["LowByteFox"],
    "version": "0.3.0",
    "sources": ["src/**"],
    "test-sources": ["test/**"],
    "output": "build",
    "targets": {
        "OpenPNGStudio-linux-x64": {
            "type": "executable",
            "name": "OpenPNGStudio",
            "linker-search-paths": ["build/miniroot/usr/lib64",
                "build/miniroot/usr/lib/x86_64-linux-gnu"],
            "linked-libraries": ["OpenPNGStudio", "raylib", "unuv", "uv", "m"]
        },
        "OpenPNGStudio-windows-x64": {
            "type": "executable",
            "name": "OpenPNGStudio",
            "linker-search-paths": ["build\\miniroot\\lib"],
            "linked-libraries": ["OpenPNGStudio", "raylib", "unuv", "libuv",
                "mman", "Advapi32", "Userenv", "Ole32", "Iphlpapi"]
        },
        "opng": {
            "type": "executable",
            "dependencies-override": [],
            "linked-libraries-override": [],
            "sources-override": ["src/opng/**"],
            "features": ["OPNG_STANDALONE"],
            "test-sources-override": ["test/opng/**"]
        }
    },
    "cpu": "generic",
    "opt": "O0",
    "features": [],
    "quiet": true
}
